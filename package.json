{
  "name": "nyhedsbrevekompilator",
  "version": "1.2.0",
  "description": "Redaktionelle nyhedsbreve kompilator",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon --inspect --ignore 'client/' --ignore 'templates/' server/index.js",
    "build": "docker build -t berlingskemedia/$npm_package_name:$npm_package_version . && docker tag -f berlingskemedia/$npm_package_name:$npm_package_version berlingskemedia/$npm_package_name:latest",
    "push": "docker push berlingskemedia/$npm_package_name:$npm_package_version && docker push berlingskemedia/$npm_package_name:latest"
  },
  "author": "Berlingske Media",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/BerlingskeMedia/nyhedsbrevekompilator"
  },
  "dependencies": {
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.6.2",
    "inert": "^4.2.1",
    "joi": "^10.6.0",
    "moment": "^2.18.1",
    "mongodb": "^2.2.36",
    "swig": "^1.4.2",
    "swig-extras": "0.0.1",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "nodemon": "^1.19.0"
  }
}
